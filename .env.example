# Database
DATABASE_URL=postgresql://username:password@localhost:5432/bikefriendly

# Server
PORT=3000
NODE_ENV=development

# CORS - comma-separated list of allowed origins
ALLOWED_ORIGINS=http://localhost:3000,https://yourdomain.com

# AdminJS Configuration (REQUIRED FOR PRODUCTION)
# SECURITY WARNING: Use strong, unique values in production
ADMIN_EMAIL=your-admin@yourdomain.com
ADMIN_PASSWORD=your-strong-password-here-min-12-chars
ADMIN_COOKIE_SECRET=your-random-secret-key-here-min-32-chars
ADMIN_JS_SKIP_BUNDLE=true
